{
  "name": "ts-stater",
  "version": "0.0.0",
  "packageManager": "pnpm@7.9.0",
  "description": "",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/guygubaby",
  "homepage": "https://github.com/guygubaby/ts-stater#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/ts-stater.git"
  },
  "bugs": {
    "url": "https://github.com/guygubaby/ts-stater/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "esno src/index.ts",
    "dev": "pnpm build -- --watch",
    "build": "tsup",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^2.0.2",
    "@types/node": "^18.6.4",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.1",
    "vite": "^3.0.4",
    "vitest": "^0.21.0"
  }
}
